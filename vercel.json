{
    "builds": [{
        "src": "build_staticfiles.sh",
        "use": "@vercel/static-build@1.0.34",
        "config": {
          "distDir": "staticfiles_build"
        }
      },
      {
        "src": "portifolio/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "portifolio/wsgi.py"
        }
    ]
}